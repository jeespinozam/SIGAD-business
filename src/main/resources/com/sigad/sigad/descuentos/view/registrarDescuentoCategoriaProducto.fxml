<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="280.0" prefWidth="432.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sigad.sigad.descuentos.controller.RegistrarDescuentoCategoriaProductoController">
    <stylesheets>
        <URL value="@/styles/registrardescuentocategoriaproducto.css" />
    </stylesheets>
    <children>
      <StackPane prefHeight="400.0" prefWidth="150.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane prefHeight="239.0" prefWidth="432.0">
               <children>
                  <JFXDatePicker fx:id="txtFechaInicio" defaultColor="#455a64" layoutX="42.0" layoutY="107.0" prefHeight="17.0" prefWidth="171.0" promptText="Fecha Inicio" />
                  <JFXDatePicker fx:id="txtFechaFin" defaultColor="#455a64" layoutX="42.0" layoutY="155.0" promptText="Fecha Fin" />
                  <JFXTextField fx:id="txtValuePct" layoutX="266.0" layoutY="56.0" prefHeight="25.0" prefWidth="135.0" promptText="Descuento (%) ">
                     <font>
                        <Font name="System Italic" size="12.0" />
                     </font>
                  </JFXTextField>
                  <JFXButton fx:id="btnGuardar" buttonType="RAISED" defaultButton="true" layoutX="266.0" layoutY="225.0" onAction="#guardarDescuento" style="-fx-background-color: #FFFFFF;" text="Guardar">
                     <effect>
                        <DropShadow color="#000000f5" />
                     </effect>
                  </JFXButton>
                  <JFXButton fx:id="btnCancelar" buttonType="RAISED" cancelButton="true" layoutX="104.0" layoutY="225.0" onMouseClicked="#close" style="-fx-background-color: #FFFFFF;" text="Cancelar">
                     <effect>
                        <DropShadow color="#000000f3" />
                     </effect>
                  </JFXButton>
                  <Label fx:id="lblError" layoutX="43.0" layoutY="187.0" prefHeight="25.0" prefWidth="184.0" />
                  <JFXComboBox fx:id="cmbCategorias" layoutX="40.0" layoutY="56.0" prefHeight="25.0" prefWidth="171.0" promptText="Categorias" />
               </children>
            </AnchorPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
